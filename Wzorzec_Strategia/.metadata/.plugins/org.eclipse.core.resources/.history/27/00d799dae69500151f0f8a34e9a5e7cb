
public class Strategy {
	
	
	
	
	public static void main(String []args){
		
		Postac knight = new Knight();
		knight.speak();
		knight.selectWeap();
		knight.changeWeapon(new Machete());
		knight.walcz();
		knight.mov();
		knight.selectWeap();
		knight.changeWeapon(new Sword());
		knight.selectWeap();
	}

}
